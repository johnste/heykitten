{
  "name": "Heykitten",
  "version": "0.0.2",
  "manifest_version": 2,
  "homepage_url": "http://github.com/johnste/heykitten",
  "description": "Replace facebook user images with kittens.",

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://fbcdn-profile-a.akamaihd.net/*",
    "*://fbcdn-photos-c-a.akamaihd.net/*",
    "*://fbcdn-sphotos-f-a.akamaihd.net/*"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}